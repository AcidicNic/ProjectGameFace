<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background track -->
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#E0E0E0" /> <!-- Disabled background color -->
        </shape>
    </item>
    <!-- Progress track -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#B0B0B0" /> <!-- Disabled progress color -->
            </shape>
        </clip>
    </item>
</layer-list>

